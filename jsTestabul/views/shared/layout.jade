                                        .navbar-inner
                                            span.badge.badge-info Badge Radius
                                    input(data-bind='value: navBindings.cto.badgeRadius')
                .page-wrap
                    header#main-header.main-header.well
                        a.open-menu(href="#main-nav")☰
                        a.close-menu(href="#")☰

                        div#page-nav(style='position: absolute; left: 70px; top: 0px; padding: 3px !important;')
                            ul.nav.nav-pills
                                li()
                                    a.well.pill-link(href='/') Tests
                                li()
                                    a.well.pill-link(href='/benchmarks',data-bind="visible: suiteView.unitTestFrameworkManager.getFramework()==='itchcork'") Benchmarks
                                li()
                                    a.well.pill-link(href='/coverage') Coverage
                        .well(style='position: absolute; right: 57px; top: 0px; padding: 3px !important;')

                            span.well.badge.badge-success.headCount(data-bind='text: totals.Passed()')
                            span.well.badge.badge-important.headCount(data-bind='text: totals.Failed(), visible: totals.Failed() > 0')
                        div(style='position: absolute; right: 0; top: 0; width: 52px; height: 44px; margin: 0; !important;border-left: 1px solid #252A30 !important; cursor: pointer;', data-bind="click: collapseAll")
                              img#rightCorkCollapse.collapseAll(title='Collapse All',style='margin: 0 15px 0 15px;', width='21', height='44', src='/img/menulogo.png')


                    div.results(style="display:none;",data-bind='visible: viewBounded')
                        block content

